{
    "name": "itacademy-sprint4",
    "version": "1.0.0",
    "license": "UNLICENSED",
    "type": "module",
    "scripts": {
        "test": "jest --watch",
        "test:watch": "jest --watchAll --verbose=false"
    },
    "devDependencies": {
        "jest": "^27.4.7",
        "jest-html-reporter": "^3.3.0",
        "jest-junit": "^12.0.0",
        "puppeteer": "^13.1.2",
        "test": "^0.6.0"
    },
    "jest": {
        "reporters": [
            "default",
            [
                "./node_modules/jest-html-reporter",
                {
                    "pageTitle": "Test Solution",
                    "outputPath": "test-results.html",
                    "includeFailureMsg": true,
                    "includeConsoleLog": true
                }
            ]
        ]
    },
    "jest-junit": {
        "suiteNameTemplate": "{filepath}",
        "classNameTemplate": "{classname}",
        "titleTemplate": "{title}"
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "none"
    },
    "dependencies": {
        "browserify": "^17.0.0"
    },
    "description": "## Introduction",
    "main": "index.js",
    "directories": {
        "test": "tests"
    },
    "keywords": [],
    "author": ""
}
